% Gemini theme
% https://github.com/anishathalye/gemini

% ====================
% Definitions
% ====================

\definecolor{labsixorange}{RGB}{231, 120, 19}

% Extra colors
\definecolor{lightgray}{RGB}{240, 240, 240}
\definecolor{UFCOrange}{RGB}{231,120,19}
\definecolor{UFCGold}{RGB}{206,177,68}
\definecolor{UFCBlue}{RGB}{0,83,134}
\definecolor{UFCGreen}{RGB}{0,146,62}
\definecolor{UFCGray}{RGB}{194,193,193}
\definecolor{UFCWhite}{RGB}{255,255,255}

% ====================
% Theme
% ====================

% Basic colors
\setbeamercolor{palette primary}{fg=black,bg=white}
\setbeamercolor{palette secondary}{fg=black,bg=white}
\setbeamercolor{palette tertiary}{bg=black,fg=white}
\setbeamercolor{palette quaternary}{fg=black,bg=white}
\setbeamercolor{structure}{fg=UFCBlue}

% Headline
\setbeamercolor{headline}{fg=black,bg=white}
\setbeamercolor{headline rule}{bg=black}

% Block
\setbeamercolor{block title}{fg=UFCBlue,bg=white}
\setbeamercolor{block separator}{bg=black}
\setbeamercolor{block body}{fg=black,bg=white}

% Alert Block
\setbeamercolor{block alerted title}{fg=UFCBlue,bg=lightgray}
\setbeamercolor{block alerted separator}{bg=black}
\setbeamercolor{block alerted body}{fg=black,bg=lightgray}

% Heading
\setbeamercolor{heading}{fg=black}

% Itemize
\setbeamercolor{item}{fg=UFCBlue}

% Bibliography
\setbeamercolor{bibliography item}{fg=black}
\setbeamercolor{bibliography entry author}{fg=black}
\setbeamercolor{bibliography entry title}{fg=black}
\setbeamercolor{bibliography entry location}{fg=black}
\setbeamercolor{bibliography entry note}{fg=black}
